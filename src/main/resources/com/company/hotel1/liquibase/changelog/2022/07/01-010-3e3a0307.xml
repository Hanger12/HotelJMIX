<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="Hotel1">
        <createTable tableName="APARTMENT">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_APARTMENT"/>
            </column>
            <column name="NUMBER_" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="SIGN_OF_BOOKING" type="BOOLEAN"/>
            <column name="SIGN_OF_EMPLOYMENT" type="BOOLEAN"/>
        </createTable>
    </changeSet>
</databaseChangeLog>